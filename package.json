{
  "name": "enhanced-pdf-redaction-app",
  "version": "2.0.0",
  "description": "Enhanced PDF redaction application with true text removal and SLDS styling",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && mkdir -p uploads public",
    "lint": "eslint *.js",
    "clean": "rm -rf uploads/* node_modules/.cache"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.57.0"
  },
  "keywords": [
    "pdf",
    "redaction",
    "text-removal",
    "security",
    "privacy",
    "node",
    "express",
    "slds",
    "salesforce-lightning"
  ],
  "author": "MS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourname/enhanced-pdf-redaction-app"
  },
  "bugs": {
    "url": "https://github.com/yourname/enhanced-pdf-redaction-app/issues"
  },
  "homepage": "https://github.com/yourname/enhanced-pdf-redaction-app#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "config": {
    "port": "3000",
    "maxFileSize": "50MB"
  }
}
